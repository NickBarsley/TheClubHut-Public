<h1>New whatshappening</h1>

<%= error_messages_for :whatshappening %>

<% form_for(@whatshappening) do |f| %>
  <p>
    <b>Club</b><br />
    <%= f.text_field :club_id %>
  </p>

  <p>
    <b>User</b><br />
    <%= f.text_field :user_id %>
  </p>

  <p>
    <b>Team</b><br />
    <%= f.text_field :team_id %>
  </p>

  <p>
    <b>Title</b><br />
    <%= f.text_field :title %>
  </p>

  <p>
    <b>Body</b><br />
    <%= f.text_field :body %>
  </p>

  <p>
    <b>Event type</b><br />
    <%= f.text_field :event_type %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', whatshappenings_path %>
